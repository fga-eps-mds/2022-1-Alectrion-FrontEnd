{
  "name": "Alectrion-frontend",
  "version": "0.1.0",
  "private": true,
  "jest": {
    "coveragePathIgnorePatterns": [
      "src/App.tsx",
      "src/index.tsx",
      "src/reportWebVitals.ts",
      "src/setupTests.ts",
      "src/routes/",
      "src/config",
      "src/context"
    ]
  },
  "@casualbot/jest-sonar-reporter": {
    "suiteName": "jest tests",
    "outputDirectory": "coverage",
    "outputName": "jest-report.xml",
    "uniqueOutputName": "false",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true",
    "relativePaths": "true"
  },
  "dependencies": {
    "@casualbot/jest-sonar-reporter": "^2.2.7",
    "@date-io/date-fns": "1.x",
    "@date-io/dayjs": "1.x",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.9.2",
    "@mui/styled-engine-sc": "^5.8.0",
    "@mui/x-date-pickers": "^5.0.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "axios": "^0.27.2",
    "axios-mock-adapter": "^1.21.2",
    "date-fns": "^2.29.2",
    "dayjs": "^1.11.5",
    "formik": "^2.2.9",
    "i": "^0.3.7",
    "jest-mock-axios": "^4.7.0-beta",
    "react": "^18.2.0",
    "react-csv": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-idle-timer": "^5.4.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.0.8",
    "serve": "^14.0.1",
    "styled-components": "^5.3.5",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "serve -s ./build/",
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "npx eslint --fix src/**/*.ts",
    "test": "react-scripts test ",
    "test:staged": "npm test --  --env=jsdom --watchAll=false --silent --findRelatedTests src/**/*.test.tsx",
    "test:all": "CI=true npm test --  --env=jsdom --silent --watchAll=false --coverage --testResultsProcessor @casualbot/jest-sonar-reporter"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "testPathIgnorePatterns": "./src/pages/EditUser/index.test.tsx"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.9",
    "@types/react-csv": "^1.1.3",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
